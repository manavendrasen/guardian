{
  "name": "cli",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "guardian": "./bin/run"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest"
  },
  "dependencies": {
    "@oclif/core": "^1.16.5",
    "@oclif/plugin-help": "^5.1.15",
    "@oclif/plugin-plugins": "^2.1.2",
    "execa": "^6.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "8.2.4",
    "inquirer-autocomplete-prompt": "2.0.0",
    "axios": "^1.1.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.3",
    "@types/chai": "^4.3.3",
    "@types/inquirer": "8.2.4",
    "@types/inquirer-autocomplete-prompt": "2.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.3",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "oclif": "^3.2.16",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:^0.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "oclif": {
    "bin": "guardian",
    "dirname": "guardian",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "types": "dist/index.d.ts"
}
